{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore.Authentication.JwtBearer": "Debug",
      "Microsoft.IdentityModel.Protocols.OpenIdConnect": "Debug",
      "System.Net.Http.HttpClient": "Information"
    }
  },
  "JwtSettings": {
    "Audience": "swagger-client",
    "MetadataAddress": "http://keycloak:8080/realms/bankaccounts/.well-known/openid-configuration",
    "ValidIssuer": "http://localhost:8081/realms/bankaccounts"
  },
  "SwaggerSettings": {
    "AuthorizationUrl": "http://localhost:8081/realms/bankaccounts/protocol/openid-connect/auth",
    "OpenIdScope": "openid",
    "ProfileScope": "profile",
    "Description": "Авторизация через Keycloak с Implicit Flow",
    "SecurityScheme": "Bearer"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=db;Username=admin;Password=admin"
  },
  "AllowedHosts": "*"
}
